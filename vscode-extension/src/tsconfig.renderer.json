{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "outDir": "../out/renderer",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": [
    "renderer.ts"
  ]
}
